# Port Configuration
BACKEND_PORT=3000
FRONTEND_PORT=3002

# API Configuration
# In development: Vite proxy forwards /api to backend
# In production: Frontend served from backend, so relative URLs work
VITE_API_URL=/api
VITE_WS_URL=/ws

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false
