# Port to run the server on
PORT=3002

# Path to the SQLite database
# Default: .sudocode/cache.db in the current working directory
SUDOCODE_DB_PATH=

# Enable/disable file watcher
# Default: true (enabled)
WATCH=true

# File watcher debounce delay in milliseconds
# Default: 2000 (2 seconds)
WATCH_DEBOUNCE=2000
